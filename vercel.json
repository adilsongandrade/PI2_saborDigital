{
  "builds": [
    {
      "src": "sabor_digital/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "sabor_digital/wsgi.py"
    }
  ],
  "installCommand": "pip install --no-cache-dir -r requirements.txt",
  "buildCommand": "python manage.py collectstatic --no-input"
}